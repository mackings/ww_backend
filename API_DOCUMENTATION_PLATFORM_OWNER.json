    {
      "name": "Create Global Product",
      "method": "POST",
      "path": "/api/platform/products/global",
      "description": "Create a global product that is instantly visible to all companies. Automatically approved.",
      "authentication": "Required (Platform Owner)",
      "headers": {
        "Authorization": "Bearer <token>",
        "Content-Type": "multipart/form-data"
      },
      "requestBody": {
        "name": {
          "type": "string",
          "required": true,
          "description": "Product name"
        },
        "category": {
          "type": "string",
          "required": true,
          "description": "Product category"
        },
        "subCategory": {
          "type": "string",
          "optional": true,
          "description": "Product subcategory"
        },
        "description": {
          "type": "string",
          "optional": true,
          "description": "Product description"
        },
        "image": {
          "type": "file",
          "optional": true,
          "description": "Product image file (JPG, PNG)"
        },
        "isGlobal": {
          "type": "boolean",
          "required": true,
          "value": true,
          "description": "Must be true for global products"
        }
      },
      "requestExample": {
        "method": "POST",
        "url": "/api/platform/products/global",
        "contentType": "multipart/form-data",
        "formData": {
          "name": "Premium Mahogany Wood Plank",
          "category": "Raw Materials",
          "subCategory": "Hardwood",
          "description": "High-quality mahogany wood planks, perfect for premium furniture projects",
          "isGlobal": "true",
          "image": "[Binary file data]"
        }
      },
