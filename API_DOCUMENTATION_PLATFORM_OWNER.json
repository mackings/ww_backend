  "dataStructures": {
    "Company": {
      "_id": "string (ObjectId)",
      "name": "string",
      "email": "string",
      "phoneNumber": "string",
      "address": "string",
      "owner": "string (ObjectId reference to User)",
      "isActive": "boolean",
      "createdAt": "string (ISO 8601 date)",
      "updatedAt": "string (ISO 8601 date)"
    },
    "Product": {
      "_id": "string (ObjectId)",
      "userId": "string (ObjectId reference to User)",
      "companyName": "string",
      "name": "string",
      "productId": "string (PRD-XXXXXX format)",
      "category": "string",
      "subCategory": "string (optional)",
      "description": "string (optional)",
      "image": "string (URL, optional)",
      "isGlobal": "boolean",
      "status": "string (enum: 'pending', 'approved', 'rejected')",
      "approvedBy": "string (ObjectId reference to User, nullable)",
      "approvedAt": "string (ISO 8601 date, nullable)",
      "rejectionReason": "string (nullable)",
      "submittedBy": "string (ObjectId reference to User)",
      "submittedAt": "string (ISO 8601 date)",
      "resubmissionCount": "number",
      "approvalHistory": "array of ApprovalHistoryItem",
      "createdAt": "string (ISO 8601 date)",
      "updatedAt": "string (ISO 8601 date)"
    },
    "ApprovalHistoryItem": {
      "action": "string (enum: 'submitted', 'approved', 'rejected', 'resubmitted')",
      "performedBy": "string (ObjectId reference to User)",
      "performedByName": "string",
      "reason": "string (nullable)",
      "timestamp": "string (ISO 8601 date)"
    },
    "User": {
      "_id": "string (ObjectId)",
      "fullname": "string",
      "email": "string",
      "phoneNumber": "string",
      "isPlatformOwner": "boolean",
      "platformOwnerSince": "string (ISO 8601 date, nullable)"
    },
    "Order": {
      "_id": "string (ObjectId)",
      "orderNumber": "string",
      "totalAmount": "number",
      "status": "string (enum: 'pending', 'in_progress', 'completed', 'cancelled', 'on_hold')",
      "paymentStatus": "string (enum: 'unpaid', 'partial', 'paid')",
      "createdAt": "string (ISO 8601 date)"
    }
  },

  "commonErrors": [
    {
      "status": 401,
      "message": "Not authorized to access this route",
      "cause": "Missing or invalid JWT token"
    },
    {
      "status": 403,
      "message": "Access denied: Platform Owner role required",
      "cause": "User is not a platform owner (isPlatformOwner: false)"
    },
    {
      "status": 404,
      "message": "Resource not found",
      "cause": "Company, Product, or other resource doesn't exist"
    },
    {
      "status": 500,
      "message": "Server error",
      "cause": "Internal server error - check server logs"
    }
  ],

