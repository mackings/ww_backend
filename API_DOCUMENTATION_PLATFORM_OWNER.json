{
  "title": "Platform Owner Dashboard API Documentation",
  "version": "1.0.0",
  "baseURL": "https://your-api-domain.com/api",
  "description": "Complete API documentation for Platform Owners (App Owners) to manage the entire system, approve products, monitor companies, and view analytics.",

  "authentication": {
    "type": "Bearer Token (JWT)",
    "header": "Authorization: Bearer <token>",
    "note": "All endpoints require platform owner authentication. User must have isPlatformOwner: true",
    "loginEndpoint": "/api/auth/signin",
    "loginExample": {
      "request": {
        "method": "POST",
        "url": "/api/auth/signin",
        "body": {
          "email": "platformowner@example.com",
          "password": "your_password"
        }
      },
      "response": {
        "success": true,
        "message": "Login successful",
        "data": {
          "user": {
            "_id": "507f1f77bcf86cd799439011",
            "fullname": "Platform Owner Name",
            "email": "platformowner@example.com",
            "isPlatformOwner": true,
            "platformOwnerSince": "2024-01-15T10:30:00.000Z"
          },
          "token": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
        }
      }
    }
  },

